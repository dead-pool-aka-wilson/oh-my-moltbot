{
  "id": "oh-my-moltbot",
  "name": "oh-my-moltbot",
  "version": "0.1.0",
  "description": "Multi-Model Orchestration - Ollama Gateway, Proxy Mode, Ultrawork",
  "configSchema": {
    "type": "object",
    "properties": {
      "enabled": {
        "type": "boolean",
        "default": true
      },
      "ultraworkEnabled": {
        "type": "boolean",
        "default": false
      },
      "gateway": {
        "type": "object",
        "properties": {
          "ollamaUrl": {
            "type": "string",
            "default": "http://localhost:11434"
          },
          "ollamaModel": {
            "type": "string",
            "default": "qwen2.5:14b"
          }
        }
      },
      "seeds": {
        "type": "object",
        "properties": {
          "directory": {
            "type": "string",
            "default": "~/Dev/personal-blog/content/.seeds"
          }
        }
      }
    }
  }
}
