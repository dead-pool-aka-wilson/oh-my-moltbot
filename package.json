{
  "name": "oh-my-moltbot",
  "version": "0.1.0",
  "description": "Multi-Model Orchestration Plugin for Moltbot - Ollama Gateway, Proxy Mode, Ultrawork",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./plugin": {
      "import": "./dist/moltbot-plugin/index.js",
      "types": "./dist/moltbot-plugin/index.d.ts"
    },
    "./gateway": {
      "import": "./dist/gateway/ollama-gateway.js"
    },
    "./proxy": {
      "import": "./dist/proxy/index.js"
    },
    "./ultrawork": {
      "import": "./dist/ultrawork/index.js"
    }
  },
  "bin": {
    "oh-my-moltbot": "./bin/oh-my-moltbot.js"
  },
  "files": [
    "dist",
    "bin",
    "README.md"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target bun --format esm && tsc --emitDeclarationOnly",
    "dev": "bun --watch src/index.ts",
    "test": "bun test",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "moltbot",
    "plugin", 
    "multi-model",
    "orchestration",
    "ollama",
    "gateway",
    "proxy",
    "ultrawork",
    "agents",
    "ai",
    "llm"
  ],
  "author": "Koed",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dead-pool-aka-wilson/oh-my-moltbot"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "moltbot": ">=0.1.0"
  },
  "peerDependenciesMeta": {
    "moltbot": {
      "optional": true
    }
  }
}
